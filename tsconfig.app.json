/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "./out-tsc/app",
    "types": []
  },
  "files": [
    "src/main.ts"
  ],
  "include": [
    "src",
    "src/app",
    "src/**/*.d.ts",
  ],
  "exclude": [
    "node_modules",
    "src/app/**/*.component.spec",
    "src/app/**/*.component.html",
    "src/app/**/*.component.scss"
  ]
}
